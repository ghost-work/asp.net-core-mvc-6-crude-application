@model List<mvcCrudeApplication.Models.Domain.Employee>
@{
}
<h1>Employee List</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Salary</th>
            <th>Date of Birth</th>
            <th>Department</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.Salary</td>
                <td>@employee.BirthDate</td>
                <td>@employee.Dept</td>
                <td><a href="Employee/View/@employee.Id">Update</a></td>
            </tr>
        }
    </tbody>
</table>